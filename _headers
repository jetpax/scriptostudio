# Cache control for GitHub Pages / Netlify / Cloudflare Pages
# This file tells the CDN not to cache index.json

/index.json
  Cache-Control: no-cache, no-store, must-revalidate
  Pragma: no-cache
  Expires: 0

# ScriptO files should be cached since they're versioned
/ScriptOs/*
  Cache-Control: public, max-age=31536000, immutable

/Apps/*
  Cache-Control: public, max-age=31536000, immutable

